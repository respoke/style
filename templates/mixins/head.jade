mixin head(options)
    - options = options ? options : {}
    - options.analyticsDomains = options.analyticsDomains ? options.analyticsDomains : []

    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(
        name='viewport'
        content='width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no'
    )
    link(rel="shortcut icon" type="image/ico" href="https://www.respoke.io/favicon.ico")
    <!--[if lt IE 9]>
    script(src='/js/html5shiv.min.js')
    <![endif]-->
    <!--[if IE]>
    link(rel="stylesheet" type="text/css" href="/css/ie.css")
    <![endif]-->
    link(type="text/css" rel='stylesheet' href='//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css')

    script.
        window._built = "!{ +new Date() }";

    script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js" defer)
    script(src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js" defer)
    script(src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js" defer)
    script(src="/js/respoke-site.js" defer)

    //- Analytics
    script(type='text/javascript').
        var allowedAnalyticsDomains = !{JSON.stringify(options.analyticsDomains)};
        if (allowedAnalyticsDomains.indexOf(location.hostname) > -1) {
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
            ga('create', 'UA-52260305-1', 'auto');
            ga('send', 'pageview');
        } else {
            ga = function noop() {
                window.console && console.log('[analytics event]', arguments);
            };
        }
